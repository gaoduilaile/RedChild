<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义标题栏-->
    <declare-styleable name="TitleBar">
        <attr name="leftIsBack" format="boolean"/>
        <attr name="titleIsPicture" format="boolean"/>
        <attr name="leftIsVisible" format="boolean"/>
        <attr name="rightIsVIsible" format="boolean"/>
        <attr name="leftTitle" format="string"/>
        <attr name="rightTitle" format="string"/>
        <attr name="mainTitle" format="string"/>
        <attr name="titleDrawable" format="reference"/>
    </declare-styleable>

    <!--更多模块 自定义条目背景-->
    <declare-styleable name="CommonItemMore">
        <!--声明左边图片资源-->
        <attr name="rightDrawable" format="reference"/>
        <!--声明条目名属性-->
        <attr name="itemName" format="string"/>
        <!--声明背景类型属性-->
        <attr name="bgType">
            <enum name="first" value="1"/>
            <enum name="middle" value="2"/>
            <enum name="last" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FreeGrowUpParentRelativeLayout">

        <attr name="offset" format="dimension|reference"/>
    </declare-styleable>


</resources>